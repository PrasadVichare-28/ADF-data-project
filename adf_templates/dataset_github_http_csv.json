{
  "name": "DS_GitHub_Transactions_CSV",
  "type": "Microsoft.DataFactory/factories/datasets",
  "properties": {
    "linkedServiceName": {
      "referenceName": "LS_GitHub_Raw",
      "type": "LinkedServiceReference"
    },
    "parameters": {
      "owner": {
        "type": "String"
      },
      "repo": {
        "type": "String"
      },
      "branch": {
        "type": "String"
      },
      "yyyymmdd": {
        "type": "String"
      }
    },
    "type": "HttpFile",
    "typeProperties": {
      "relativeUrl": "@{concat('/', pipeline().parameters.owner, '/', pipeline().parameters.repo, '/', pipeline().parameters.branch, '/data/daily/transactions_', dataset().yyyymmdd, '.csv')}"
    }
  }
}